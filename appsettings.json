{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "gRPC": {
          "Url": "http://0.0.0.0:5002",
          "Protocols": "Http2"
      },
      "REST": {
        "Url": "http://0.0.0.0:5001",
        "Protocols": "Http1"
      }
    }
  },
  "Jwt": {
    "AccessTokenMinutesToExist": 60,
    "RefreshTokenMinutesToExist": 43200,
    "AccessAudience": "A",
    "RefreshAudience": "R"
  }
}
